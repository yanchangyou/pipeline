<?xml version="1.0"?>

<digester-rules>
	<!-- 流程模型:并行:parallel-->
	<pattern value="*/parallel">
		<object-create-rule classname="org.software.sphere.society.platform.pipeline.core.flow.parallel.Parallel" />
		<set-properties-rule />
		<set-next-rule methodname="appendFlow" paramtype="org.software.sphere.society.platform.pipeline.core.flow.FlowNode" />
	</pattern>
</digester-rules>
