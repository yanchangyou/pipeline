<?xml version="1.0"?>

<digester-rules>
	<pattern value="http_1.1">
		<object-create-rule classname="org.software.sphere.society.platform.pipeline.core.flow.unit.HTTP_1_1" />
		<set-properties-rule />

		<!-- 导入 var-define-param-request-response的 digestor文件 -->
		<include path="org/software/sphere/society/platform/pipeline/core/lang/execute/digestor/var-define-param-request-response-digester-rules.xml"/>

		<set-next-rule methodname="appendFlow" paramtype="org.software.sphere.society.platform.pipeline.core.flow.FlowNode" />
	</pattern>
	
	
	<pattern value="telnet">
		<object-create-rule classname="org.software.sphere.society.platform.pipeline.core.flow.unit.TELNET" />
		<set-properties-rule />

		<!-- 导入 var-define-param-request-response的 digestor文件 -->
		<include path="org/software/sphere/society/platform/pipeline/core/lang/execute/digestor/var-define-param-request-response-digester-rules.xml"/>

		<set-next-rule methodname="appendFlow" paramtype="org.software.sphere.society.platform.pipeline.core.flow.FlowNode" />
	</pattern>

	
	<pattern value="ftp">
		<object-create-rule classname="org.software.sphere.society.platform.pipeline.core.flow.unit.FTP" />
		<set-properties-rule />

		<!-- 导入 var-define-param-request-response的 digestor文件 -->
		<include path="org/software/sphere/society/platform/pipeline/core/lang/execute/digestor/var-define-param-request-response-digester-rules.xml"/>

		<set-next-rule methodname="appendFlow" paramtype="org.software.sphere.society.platform.pipeline.core.flow.FlowNode" />
	</pattern>
	
	
</digester-rules>
