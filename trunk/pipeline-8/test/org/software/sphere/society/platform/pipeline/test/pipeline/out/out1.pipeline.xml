<?xml version="1.0"?>
<root>
	<global name="self">
		<data-node-define>
			<string name="var">true</string>
		</data-node-define>
		<service-node-define>
			<service name="hello-service">
				<competitor name="output-service" realService="self.self.self.self.io-server.output-telnet1" priority="2" />
			</service>
		</service-node-define>

		<economy name="self">
			<data-node-define>
				<string name="var">true</string>
			</data-node-define>
			<service-node-define>
				<service name="hello-service">
					<competitor name="output-service" realService="self.self.self.self.io-server.output-telnet1" priority="2" />
				</service>
			</service-node-define>

			<market name="self">
				<data-node-define>
					<string name="var">true</string>
				</data-node-define>
				<service-node-define>
					<service name="hello-service">
						<competitor name="output-service" realService="self.self.self.self.io-server.output-telnet1" priority="2" />
					</service>
				</service-node-define>

				<supplier name="self">
					<data-node-define>
						<string name="var">true</string>
					</data-node-define>
					<service-node-define>
						<service name="hello-service">
							<competitor name="output-service" realService="self.self.self.self.io-server.output-telnet1" priority="2" />
						</service>
					</service-node-define>

					<server name="self">
						<data-node-define>
							<string name="var">true</string>
						</data-node-define>
						<service-node-define>
							<service name="hello-service">
								<competitor name="output-service" realService="self.self.self.self.io-server.output-telnet1" priority="2" />
							</service>
						</service-node-define>

						<telnet name="self" service="hello-service@self.self.self.self.self">
							<request name="request">
								<param name="data">pipeline speak : hello world!</param>
							</request>
						</telnet>
					</server>
					<server name="io-server" host="127.0.0.1">
						<telnet name="output-telnet1">
							<param name="host">127.0.0.1</param>
							<param name="port">10001</param>
						</telnet>
						<telnet name="output-telnet2">
							<param name="host">127.0.0.1</param>
							<param name="port">10002</param>
						</telnet>
					</server>
				</supplier>
			</market>
		</economy>
	</global>
</root>