<?xml version="1.0"?>

<digester-rules>
	<pattern value="demo-list">
		<object-create-rule classname="org.software.matter.molecule.platform.pipeline.demo.Demo" />
		<pattern value="demo">
			<object-create-rule classname="org.software.matter.molecule.platform.pipeline.demo.Demo" />
			<set-properties-rule />
			<pattern value="function">
				<pattern value="line">
					<call-method-rule methodname="addFunctionLine" paramcount="0" />
				</pattern>
			</pattern>
			<pattern value="inner-flow">
				<pattern value="line">
					<call-method-rule methodname="addInnerFlowLine" paramcount="0" />
				</pattern>
			</pattern>
			<pattern value="outer-flow">
				<pattern value="line">
					<call-method-rule methodname="addOuterFlowLine" paramcount="0" />
				</pattern>
			</pattern>
			<set-next-rule methodname="addDemo" paramtype="org.software.matter.molecule.platform.pipeline.demo.Demo" />
		</pattern>
	</pattern>
</digester-rules>
