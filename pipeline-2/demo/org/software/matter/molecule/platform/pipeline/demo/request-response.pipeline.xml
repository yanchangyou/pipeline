<?xml version="1.0"?>


<economy>
	<market name="南方市场" region="">
		<supplier name="软通">
			<server name="邮件服务器">
				<service name="main" type="public" port="10000">
					<request>
						<input>
							<![CDATA[<message>Hello World!</message>]]>
						</input>
					</request>
					<response>
						<output to="localhost.console"></output>
					</response>
					<pipeline>
						<line>
							<step service="service" server="server" supplier="supplier"
								market="market">
								<request>
									<xml-input>
										<![CDATA[<message>Hello World!</message>]]>
									</xml-input>
								</request>
							</step>
						</line>
					</pipeline>
				</service>
			</server>
			<server name="printServer">
				<meta>
					<property name="host" value="127.0.0.1"></property>
					<property name="protocol" value="ftp"></property>
				</meta>
				<service name="print" port="10000"></service>
			</server>
		</supplier>
	</market>
</economy>
