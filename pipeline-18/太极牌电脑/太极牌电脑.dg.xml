<?xml version="1.0" encoding="UTF-8"?>

<digester-rules>

	<pattern value="太极牌电脑">
		<set-properties-rule/>
		
		<pattern value="太极系统">
			<object-create-rule classname="太极.太极系统" />
			<set-properties-rule/>
			
			<pattern value="太极服务">
				<object-create-rule classname="太极.太极服务" />
				<set-properties-rule/>
				
				<set-next-rule methodname="添加服务" />
			</pattern>
			<set-next-rule methodname="安装系统" />
		</pattern>
	</pattern>
	
</digester-rules>
